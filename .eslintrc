{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"object-curly-spacing":"off",
		"arrow-parens": "off",
		"require-jsdoc": "off",
    "space-before-blocks": "off",
    "no-trailing-spaces": "off",
    "padded-blocks": "off",
    "indent": "off"
	},
	"extends": ["eslint:recommended", "google"]
}